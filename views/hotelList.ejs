        </nav>
      </div>
    </div>
  </header>

  <main>

    <h1 class="fadeUp1"><%= title %></h1>

    <div id="registHotelInfo" hidden>
      <%= JSON.stringify(values) %>
    </div>
    <div id="registHotelList" class="regUser">
      <form action="/" method="post">
        <table id="hTable">
          <tbody id="uHotelTbody">
          </tbody>
        </table>
