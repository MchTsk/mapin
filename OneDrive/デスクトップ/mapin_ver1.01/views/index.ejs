<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://maps.google.com/maps/api/js?key=AIzaSyAPggR6p__IQt-bL-pMxJ9Wb987XYuTitg&language=ja"></script>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <script type="text/javascript" src="/javascripts/commonCustom.js"></script>
  <script type="text/javascript" src="/javascripts/indexCustom.js"></script>

  <style>
    html { height: 50% }
    body { height: 100% }
    #map { height: 100%; width: 100%}
  </style>

</head>

<body>
  <h1><%= title %></h1>

  <div class="regUser">ようこそ、 <span style="font-weight: bold;"><%= userName %></span> さん</div>
  <div class="outUser">ログアウトしました。</div>

  <div class="nonUser"><a id="regist_link" href="">会員登録はこちら</a></div>
  <div class="nonUser"><a id="login_link" href="">ログイン</a></div>
  <div class="regUser"><a id="logout_link" href="">ログアウト</a></div>
  <div class="regUser"><a id="form_link" href="">ホテルのマップ情報登録はこちら</a></div>

  <div>
    <button class="search" type="button" aria-haspopup="menu" aria-label="" onclick="clickSearch('current')">
        <span>🔍現在地から検索</span>
    </button>
  </div>
  <div id="mapInfo" hidden>
    <%= JSON.stringify(values) %>
  </div>
  <div id="map" hidden></div>

  <div id="registHotelInfo" hidden>
    <%= JSON.stringify(usersVal) %>
  </div>
  <div id="registHotelList" class="regUser">
    <form action="/" method="post">
      <table id="hTable">
        <tbody id="uHotelTbody">
          <tr>
            <h3 id="hListTitle" style="margin-top: 20px; margin-bottom: 5px;">◆ホテル一覧</h3>
          </tr>
        </tbody>
      </table>
    </form>
  </div>

</body>

</html>
